{% extends 'base.html.twig' %}

{% block title %}City{% endblock %}

{% block body %}
    <h1>City</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ city.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ city.name }}</td>
            </tr>
            <tr>
                <th>Department</th>
                <td>{{ city.department }}</td>
            </tr>
            <tr>
                <th>Insee</th>
                <td>{{ city.insee }}</td>
            </tr>
            <tr>
                <th>Zip</th>
                <td>{{ city.zip }}</td>
            </tr>
            <tr>
                <th>Lat</th>
                <td>{{ city.lat }}</td>
            </tr>
            <tr>
                <th>Lng</th>
                <td>{{ city.lng }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_city_index') }}">back to list</a>

    <a href="{{ path('admin_city_edit', {'id': city.id}) }}">edit</a>

    {{ include('admin/city/_delete_form.html.twig') }}
{% endblock %}
